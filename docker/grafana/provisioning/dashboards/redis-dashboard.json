{
  "dashboard": {
    "id": null,
    "title": "GridTokenX Redis Monitoring",
    "tags": ["redis", "gridtokenx", "cache"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Redis Memory Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_memory_used_bytes{instance=\"redis:6379\"}",
            "legendFormat": "Used Memory"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1073741824},
                {"color": "red", "value": 2147483648}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Connected Clients",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_connected_clients{instance=\"redis:6379\"}",
            "legendFormat": "Connected Clients"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 80}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Operations per Second",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Ops/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8}
      },
      {
        "id": 5,
        "title": "Redis CPU Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(redis_cpu_sys_seconds_total[5m]) * 100",
            "legendFormat": "CPU %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 80}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8}
      },
      {
        "id": 6,
        "title": "Memory Usage Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "redis_memory_used_bytes{instance=\"redis:6379\"}",
            "legendFormat": "Used Memory"
          },
          {
            "expr": "redis_memory_max_bytes{instance=\"redis:6379\"}",
            "legendFormat": "Max Memory"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Cache Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Key Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (key_type) (redis_db_keys{instance=\"redis:6379\"})",
            "legendFormat": "{{key_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 9,
        "title": "Expired Keys",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(redis_expired_keys_total[5m])",
            "legendFormat": "Expired Keys/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 10,
        "title": "Network I/O",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(redis_net_input_bytes_total[5m])",
            "legendFormat": "Network Input"
          },
          {
            "expr": "rate(redis_net_output_bytes_total[5m])",
            "legendFormat": "Network Output"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40}
      },
      {
        "id": 11,
        "title": "Slow Queries",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_slowlog_length{instance=\"redis:6379\"}",
            "legendFormat": "Slow Log Length"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48}
      },
      {
        "id": 12,
        "title": "Pub/Sub Channels",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_pubsub_channels{instance=\"redis:6379\"}",
            "legendFormat": "Active Channels"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
